function createMainGUI()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "MainGUI"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
    
    local mainFrame = Instance.new("Frame")
    mainFrame.Name = "MainFrame"
    mainFrame.Size = UDim2.new(0, 500, 0, 350)
    mainFrame.Position = UDim2.new(0.5, -250, 0.5, -175)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = screenGui
    
    local mainCorner = Instance.new("UICorner")
    mainCorner.CornerRadius = UDim.new(0, 25)
    mainCorner.Parent = mainFrame
    
    local mainGradient = Instance.new("UIGradient")
    mainGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 223, 100)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 250, 200)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255))
    }
    mainGradient.Rotation = 135
    mainGradient.Parent = mainFrame
    
    local outerGlow = Instance.new("ImageLabel")
    outerGlow.Name = "OuterGlow"
    outerGlow.Size = UDim2.new(1, 100, 1, 100)
    outerGlow.Position = UDim2.new(0.5, -50, 0.5, -50)
    outerGlow.AnchorPoint = Vector2.new(0.5, 0.5)
    outerGlow.BackgroundTransparency = 1
    outerGlow.Image = "rbxassetid://5028857084"
    outerGlow.ImageColor3 = Color3.fromRGB(255, 215, 0)
    outerGlow.ImageTransparency = 0.3
    outerGlow.ScaleType = Enum.ScaleType.Slice
    outerGlow.SliceCenter = Rect.new(24, 24, 276, 276)
    outerGlow.ZIndex = 0
    outerGlow.Parent = mainFrame
    
    local particles = Instance.new("ParticleEmitter")
    particles.Parent = mainFrame
    particles.Texture = "rbxassetid://6490035152"
    particles.Rate = 20
    particles.Lifetime = NumberRange.new(2, 4)
    particles.Speed = NumberRange.new(10, 30)
    particles.SpreadAngle = Vector2.new(360, 360)
    particles.Color = ColorSequence.new(Color3.fromRGB(255, 215, 0))
    particles.Size = NumberSequence.new(0.5, 0)
    particles.Transparency = NumberSequence.new(0, 1)
    particles.LightEmission = 1
    particles.Enabled = false
    
    spawn(function()
        while mainFrame.Parent do
            local glowTween = TweenService:Create(outerGlow, TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
                ImageTransparency = 0.6
            })
            glowTween:Play()
            
            local rotateTween = TweenService:Create(mainGradient, TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1), {
                Rotation = mainGradient.Rotation + 360
            })
            rotateTween:Play()
            
            wait()
        end
    end)
    
    local uiStroke = Instance.new("UIStroke")
    uiStroke.Color = Color3.fromRGB(255, 215, 0)
    uiStroke.Thickness = 3
    uiStroke.Transparency = 0.3
    uiStroke.Parent = mainFrame
    
    spawn(function()
        while mainFrame.Parent do
            for i = 0, 1, 0.01 do
                uiStroke.Transparency = 0.3 + (math.sin(i * math.pi * 2) * 0.2)
                wait(0.03)
            end
        end
    end)
    
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, 0, 0, 70)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = "‚ú® HEAVENLY PANEL ‚ú®"
    titleLabel.TextColor3 = Color3.fromRGB(255, 215, 0)
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextSize = 32
    titleLabel.TextStrokeTransparency = 0.5
    titleLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
    titleLabel.Parent = mainFrame
    
    spawn(function()
        while titleLabel.Parent do
            for i = 28, 36 do
                titleLabel.TextSize = i
                wait(0.05)
            end
            for i = 36, 28, -1 do
                titleLabel.TextSize = i
                wait(0.05)
            end
        end
    end)
    
    local divider = Instance.new("Frame")
    divider.Size = UDim2.new(1, -40, 0, 2)
    divider.Position = UDim2.new(0, 20, 0, 75)
    divider.BackgroundColor3 = Color3.fromRGB(255, 215, 0)
    divider.BorderSizePixel = 0
    divider.Parent = mainFrame
    
    local dividerGradient = Instance.new("UIGradient")
    dividerGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 215, 0)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 255, 255)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 215, 0))
    }
    dividerGradient.Parent = divider
    
    local joinServerBtn = Instance.new("TextButton")
    joinServerBtn.Name = "JoinServerBtn"
    joinServerBtn.Size = UDim2.new(0, 400, 0, 80)
    joinServerBtn.Position = UDim2.new(0.5, -200, 0, 110)
    joinServerBtn.BackgroundColor3 = Color3.fromRGB(255, 215, 0)
    joinServerBtn.Text = "‚ö° JOIN CURRENT SERVER ‚ö°"
    joinServerBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    joinServerBtn.Font = Enum.Font.GothamBold
    joinServerBtn.TextSize = 24
    joinServerBtn.BorderSizePixel = 0
    joinServerBtn.Parent = mainFrame
    
    local joinBtnCorner = Instance.new("UICorner")
    joinBtnCorner.CornerRadius = UDim.new(0, 15)
    joinBtnCorner.Parent = joinServerBtn
    
    local joinBtnStroke = Instance.new("UIStroke")
    joinBtnStroke.Color = Color3.fromRGB(255, 255, 255)
    joinBtnStroke.Thickness = 2
    joinBtnStroke.Parent = joinServerBtn
    
    local joinBtnGradient = Instance.new("UIGradient")
    joinBtnGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 215, 0)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 150))
    }
    joinBtnGradient.Rotation = 90
    joinBtnGradient.Parent = joinServerBtn
    
    local joinGlow = Instance.new("ImageLabel")
    joinGlow.Size = UDim2.new(1, 40, 1, 40)
    joinGlow.Position = UDim2.new(0.5, -20, 0.5, -20)
    joinGlow.AnchorPoint = Vector2.new(0.5, 0.5)
    joinGlow.BackgroundTransparency = 1
    joinGlow.Image = "rbxassetid://5028857084"
    joinGlow.ImageColor3 = Color3.fromRGB(255, 255, 0)
    joinGlow.ImageTransparency = 0.5
    joinGlow.ScaleType = Enum.ScaleType.Slice
    joinGlow.SliceCenter = Rect.new(24, 24, 276, 276)
    joinGlow.ZIndex = 0
    joinGlow.Parent = joinServerBtn
    
    joinServerBtn.MouseEnter:Connect(function()
        TweenService:Create(joinServerBtn, TweenInfo.new(0.2), {Size = UDim2.new(0, 420, 0, 85)}):Play()
        TweenService:Create(joinGlow, TweenInfo.new(0.2), {ImageTransparency = 0.2}):Play()
    end)
    
    joinServerBtn.MouseLeave:Connect(function()
        TweenService:Create(joinServerBtn, TweenInfo.new(0.2), {Size = UDim2.new(0, 400, 0, 80)}):Play()
        TweenService:Create(joinGlow, TweenInfo.new(0.2), {ImageTransparency = 0.5}):Play()
    end)
    
    joinServerBtn.MouseButton1Click:Connect(function()
        local success, result = pcall(function()
            return httpRequest({
                Url = railwayURL .. "/getjobid",
                Method = "GET"
            })
        end)
        
        if success and result.StatusCode == 200 then
            local data = HttpService:JSONDecode(result.Body)
            local jobId = data.jobId
            
            TeleportService:TeleportToPlaceInstance(109983668079237, jobId, LocalPlayer)
        else
            joinServerBtn.Text = "‚ùå NO SERVER FOUND"
            wait(2)
            joinServerBtn.Text = "‚ö° JOIN CURRENT SERVER ‚ö°"
        end
    end)
    
    local ownerCmdsBtn = Instance.new("TextButton")
    ownerCmdsBtn.Name = "OwnerCmdsBtn"
    ownerCmdsBtn.Size = UDim2.new(0, 200, 0, 50)
    ownerCmdsBtn.Position = UDim2.new(1, -220, 0, 20)
    ownerCmdsBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
    ownerCmdsBtn.Text = "üëë OWNER CMDS"
    ownerCmdsBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    ownerCmdsBtn.Font = Enum.Font.GothamBold
    ownerCmdsBtn.TextSize = 16
    ownerCmdsBtn.BorderSizePixel = 0
    ownerCmdsBtn.Parent = mainFrame
    
    local ownerBtnCorner = Instance.new("UICorner")
    ownerBtnCorner.CornerRadius = UDim.new(0, 12)
    ownerBtnCorner.Parent = ownerCmdsBtn
    
    local ownerBtnGradient = Instance.new("UIGradient")
    ownerBtnGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 50, 50)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 150, 150))
    }
    ownerBtnGradient.Rotation = 45
    ownerBtnGradient.Parent = ownerCmdsBtn
    
    ownerCmdsBtn.MouseButton1Click:Connect(function()
        if isOwner(LocalPlayer.Name) then
            createOwnerGUI(screenGui, mainFrame)
        else
            createJumpscareGUI(screenGui)
        end
    end)
    
    local normalCmdsBtn = Instance.new("TextButton")
    normalCmdsBtn.Name = "NormalCmdsBtn"
    normalCmdsBtn.Size = UDim2.new(0, 200, 0, 50)
    normalCmdsBtn.Position = UDim2.new(1, -220, 0, 280)
    normalCmdsBtn.BackgroundColor3 = Color3.fromRGB(100, 200, 255)
    normalCmdsBtn.Text = "üè† NORMAL CMDS"
    normalCmdsBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    normalCmdsBtn.Font = Enum.Font.GothamBold
    normalCmdsBtn.TextSize = 16
    normalCmdsBtn.BorderSizePixel = 0
    normalCmdsBtn.Visible = false
    normalCmdsBtn.Parent = mainFrame
    
    local normalBtnCorner = Instance.new("UICorner")
    normalBtnCorner.CornerRadius = UDim.new(0, 12)
    normalBtnCorner.Parent = normalCmdsBtn
    
    mainFrame.NormalCmdsBtn = normalCmdsBtn
end

function createJumpscareGUI(parentGui)
    local jumpscareGui = Instance.new("ScreenGui")
    jumpscareGui.Name = "JumpscareGUI"
    jumpscareGui.ResetOnSpawn = false
    jumpscareGui.Parent = parentGui.Parent
    
    local jumpFrame = Instance.new("Frame")
    jumpFrame.Size = UDim2.new(1, 0, 1, 0)
    jumpFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    jumpFrame.BorderSizePixel = 0
    jumpFrame.Parent = jumpscareGui
    
    local jumpText = Instance.new("TextLabel")
    jumpText.Size = UDim2.new(1, 0, 1, 0)
    jumpText.BackgroundTransparency = 1
    jumpText.Text = "NO ‚ùå‚ùå"
    jumpText.TextColor3 = Color3.fromRGB(255, 255, 255)
    jumpText.Font = Enum.Font.GothamBold
    jumpText.TextSize = 200
    jumpText.Parent = jumpFrame
    
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://6895079853"
    sound.Volume = 1
    sound.Parent = jumpscareGui
    sound:Play()
    
    for i = 1, 10 do
        jumpFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        jumpText.Rotation = math.random(-30, 30)
        jumpFrame.Position = UDim2.new(0, math.random(-20, 20), 0, math.random(-20, 20))
        wait(0.1)
        jumpFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        wait(0.1)
    end
    
    jumpscareGui:Destroy()
end

function createOwnerGUI(parentGui, mainFrame)
    local ownerFrame = Instance.new("Frame")
    ownerFrame.Name = "OwnerFrame"
    ownerFrame.Size = UDim2.new(0, 600, 0, 450)
    ownerFrame.Position = UDim2.new(0.5, 300, 0.5, -225)
    ownerFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    ownerFrame.BorderSizePixel = 0
    ownerFrame.Parent = parentGui
    
    local ownerCorner = Instance.new("UICorner")
    ownerCorner.CornerRadius = UDim.new(0, 25)
    ownerCorner.Parent = ownerFrame
    
    local ownerGradient = Instance.new("UIGradient")
    ownerGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(100, 50, 150)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(150, 100, 200)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(200, 150, 255))
    }
    ownerGradient.Rotation = 135
    ownerGradient.Parent = ownerFrame
    
    local ownerGlow = Instance.new("ImageLabel")
    ownerGlow.Size = UDim2.new(1, 100, 1, 100)
    ownerGlow.Position = UDim2.new(0.5, -50, 0.5, -50)
    ownerGlow.AnchorPoint = Vector2.new(0.5, 0.5)
    ownerGlow.BackgroundTransparency = 1
    ownerGlow.Image = "rbxassetid://5028857084"
    ownerGlow.ImageColor3 = Color3.fromRGB(150, 100, 255)
    ownerGlow.ImageTransparency = 0.4
    ownerGlow.ScaleType = Enum.ScaleType.Slice
    ownerGlow.SliceCenter = Rect.new(24, 24, 276, 276)
    ownerGlow.ZIndex = 0
    ownerGlow.Parent = ownerFrame
    
    TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Position = UDim2.new(0.5, -550, 0.5, -175)
    }):Play()
    
    TweenService:Create(ownerFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Position = UDim2.new(0.5, -300, 0.5, -225)
    }):Play()
    
    local ownerTitle = Instance.new("TextLabel")
    ownerTitle.Size = UDim2.new(1, 0, 0, 70)
    ownerTitle.BackgroundTransparency = 1
    ownerTitle.Text = "üëë OWNER COMMANDS üëë"
    ownerTitle.TextColor3 = Color3.fromRGB(255, 215, 0)
    ownerTitle.Font = Enum.Font.GothamBold
    ownerTitle.TextSize = 28
    ownerTitle.TextStrokeTransparency = 0.5
    ownerTitle.Parent = ownerFrame
    
    local bringAllBtn = Instance.new("TextButton")
    bringAllBtn.Size = UDim2.new(0, 550, 0, 60)
    bringAllBtn.Position = UDim2.new(0, 25, 0, 90)
    bringAllBtn.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
    bringAllBtn.Text = "üåç BRING ALL"
    bringAllBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    bringAllBtn.Font = Enum.Font.GothamBold
    bringAllBtn.TextSize = 22
    bringAllBtn.BorderSizePixel = 0
    bringAllBtn.Parent = ownerFrame
    
    local bringAllCorner = Instance.new("UICorner")
    bringAllCorner.CornerRadius = UDim.new(0, 12)
    bringAllCorner.Parent = bringAllBtn
    
    bringAllBtn.MouseButton1Click:Connect(function()
        local myJobId = game.JobId
        
        pcall(function()
            httpRequest({
                Url = railwayURL .. "/update",
                Method = "POST",
                Headers = { ["Content-Type"] = "application/json" },
                Body = HttpService:JSONEncode({
                    jobId = myJobId,
                    username = LocalPlayer.Name
                })
            })
        end)
        
        bringAllBtn.Text = "‚úÖ BRINGING EVERYONE..."
        wait(2)
        bringAllBtn.Text = "üåç BRING ALL"
    end)
    
    local bringContainer = Instance.new("Frame")
    bringContainer.Size = UDim2.new(0, 550, 0, 60)
    bringContainer.Position = UDim2.new(0, 25, 0, 160)
    bringContainer.BackgroundTransparency = 1
    bringContainer.Parent = ownerFrame
    
    local bringTextBox = Instance.new("TextBox")
    bringTextBox.Size = UDim2.new(0, 370, 1, 0)
    bringTextBox.Position = UDim2.new(0, 0, 0, 0)
    bringTextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    bringTextBox.Text = ""
    bringTextBox.PlaceholderText = "Enter username..."
    bringTextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
    bringTextBox.Font = Enum.Font.Gotham
    bringTextBox.TextSize = 18
    bringTextBox.BorderSizePixel = 0
    bringTextBox.Parent = bringContainer
    
    local bringBoxCorner = Instance.new("UICorner")
    bringBoxCorner.CornerRadius = UDim.new(0, 12)
    bringBoxCorner.Parent = bringTextBox
    
    local bringBtn = Instance.new("TextButton")
    bringBtn.Size = UDim2.new(0, 170, 1, 0)
    bringBtn.Position = UDim2.new(0, 380, 0, 0)
    bringBtn.BackgroundColor3 = Color3.fromRGB(100, 200, 255)
    bringBtn.Text = "üéØ BRING"
    bringBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    bringBtn.Font = Enum.Font.GothamBold
    bringBtn.TextSize = 20
    bringBtn.BorderSizePixel = 0
    bringBtn.Parent = bringContainer
    
    local bringBtnCorner = Instance.new("UICorner")
    bringBtnCorner.CornerRadius = UDim.new(0, 12)
    bringBtnCorner.Parent = bringBtn
    
    bringBtn.MouseButton1Click:Connect(function()
        local targetUser = bringTextBox.Text
        if targetUser ~= "" then
            bringBtn.Text = "‚úÖ SENT"
            wait(1)
            bringBtn.Text = "üéØ BRING"
        end
    end)
    
    local kickBtn = Instance.new("TextButton")
    kickBtn.Size = UDim2.new(0, 550, 0, 60)
    kickBtn.Position = UDim2.new(0, 25, 0, 230)
    kickBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
    kickBtn.Text = "üë¢ KICK USER"
    kickBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    kickBtn.Font = Enum.Font.GothamBold
    kickBtn.TextSize = 22
    kickBtn.BorderSizePixel = 0
    kickBtn.Parent = ownerFrame
    
    local kickBtnCorner = Instance.new("UICorner")
    kickBtnCorner.CornerRadius = UDim.new(0, 12)
    kickBtnCorner.Parent = kickBtn
    
    kickBtn.MouseButton1Click:Connect(function()
        createUserListGUI(parentGui, ownerFrame)
    end)
    
    local changeIdBtn = Instance.new("TextButton")
    changeIdBtn.Size = UDim2.new(0, 550, 0, 60)
    changeIdBtn.Position = UDim2.new(0, 25, 0, 300)
    changeIdBtn.BackgroundColor3 = Color3.fromRGB(255, 200, 50)
    changeIdBtn.Text = "üîÑ CHANGE JOB ID"
    changeIdBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    changeIdBtn.Font = Enum.Font.GothamBold
    changeIdBtn.TextSize = 22
    changeIdBtn.BorderSizePixel = 0
    changeIdBtn.Parent = ownerFrame
    
    local changeIdCorner = Instance.new("UICorner")
    changeIdCorner.CornerRadius = UDim.new(0, 12)
    changeIdCorner.Parent = changeIdBtn
    
    changeIdBtn.MouseButton1Click:Connect(function()
        local myJobId = game.JobId
        
        pcall(function()
            httpRequest({
                Url = railwayURL .. "/update",
                Method = "POST",
                Headers = { ["Content-Type"] = "application/json" },
                Body = HttpService:JSONEncode({
                    jobId = myJobId,
                    username = LocalPlayer.Name
                })
            })
        end)
        
        changeIdBtn.Text = "‚úÖ JOB ID CHANGED"
        wait(2)
        changeIdBtn.Text = "üîÑ CHANGE JOB ID"
    end)
    
    local normalCmdsBtn = Instance.new("TextButton")
    normalCmdsBtn.Size = UDim2.new(0, 200, 0, 50)
    normalCmdsBtn.Position = UDim2.new(1, -220, 0, 20)
    normalCmdsBtn.BackgroundColor3 = Color3.fromRGB(100, 200, 255)
    normalCmdsBtn.Text = "üè† NORMAL CMDS"
    normalCmdsBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    normalCmdsBtn.Font = Enum.Font.GothamBold
    normalCmdsBtn.TextSize = 16
    normalCmdsBtn.BorderSizePixel = 0
    normalCmdsBtn.Parent = ownerFrame
    
    local normalCmdsBtnCorner = Instance.new("UICorner")
    normalCmdsBtnCorner.CornerRadius = UDim.new(0, 12)
    normalCmdsBtnCorner.Parent = normalCmdsBtn
    
    normalCmdsBtn.MouseButton1Click:Connect(function()
        TweenService:Create(ownerFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Position = UDim2.new(0.5, 600, 0.5, -225)
        }):Play()
        
        TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Position = UDim2.new(0.5, -250, 0.5, -175)
        }):Play()
        
        wait(0.5)
        ownerFrame:Destroy()
    end)
end

function createUserListGUI(parentGui, ownerFrame)
    local userListFrame = Instance.new("Frame")
    userListFrame.Size = UDim2.new(0, 500, 0, 600)
    userListFrame.Position = UDim2.new(0.5, -250, 0.5, -300)
    userListFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    userListFrame.BorderSizePixel = 0
    userListFrame.Parent = parentGui
    
    local listCorner = Instance.new("UICorner")
    listCorner.CornerRadius = UDim.new(0, 20)
    listCorner.Parent = userListFrame
    
    local listGradient = Instance.new("UIGradient")
    listGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(50, 50, 50)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(100, 100, 100))
    }
    listGradient.Rotation = 90
    listGradient.Parent = userListFrame
    
    local listTitle = Instance.new("TextLabel")
    listTitle.Size = UDim2.new(1, 0, 0, 60)
    listTitle.BackgroundTransparency = 1
    listTitle.Text = "üë• ACTIVE USERS"
    listTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    listTitle.Font = Enum.Font.GothamBold
    listTitle.TextSize = 24
    listTitle.Parent = userListFrame
    
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Size = UDim2.new(1, -40, 1, -130)
    scrollFrame.Position = UDim2.new(0, 20, 0, 70)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 8
    scrollFrame.Parent = userListFrame
    
    local success, result = pcall(function()
        return httpRequest({
            Url = railwayURL .. "/session/list",
            Method = "GET"
        })
    end)
    
    if success and result.StatusCode == 200 then
        local data = HttpService:JSONDecode(result.Body)
        local sessions = data.sessions
        
        for i, session in ipairs(sessions) do
            local userCard = Instance.new("Frame")
            userCard.Size = UDim2.new(1, -10, 0, 100)
            userCard.Position = UDim2.new(0, 0, 0, (i - 1) * 110)
            userCard.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            userCard.BorderSizePixel = 0
            userCard.Parent = scrollFrame
            
            local cardCorner = Instance.new("UICorner")
            cardCorner.CornerRadius = UDim.new(0, 12)
            cardCorner.Parent = userCard
            
            local avatar = Instance.new("ImageLabel")
            avatar.Size = UDim2.new(0, 80, 0, 80)
            avatar.Position = UDim2.new(0, 10, 0, 10)
            avatar.BackgroundTransparency = 1
            avatar.Image = session.avatar
            avatar.Parent = userCard
            
            local avatarCorner = Instance.new("UICorner")
            avatarCorner.CornerRadius = UDim.new(1, 0)
            avatarCorner.Parent = avatar
            
            local usernameLabel = Instance.new("TextLabel")
            usernameLabel.Size = UDim2.new(0, 250, 0, 30)
            usernameLabel.Position = UDim2.new(0, 100, 0, 10)
            usernameLabel.BackgroundTransparency = 1
            usernameLabel.Text = session.username
            usernameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            usernameLabel.Font = Enum.Font.GothamBold
            usernameLabel.TextSize = 18
            usernameLabel.TextXAlignment = Enum.TextXAlignment.Left
            usernameLabel.Parent = userCard
            
            local kickUserBtn = Instance.new("TextButton")
            kickUserBtn.Size = UDim2.new(0, 100, 0, 35)
            kickUserBtn.Position = UDim2.new(1, -110, 0.5, -17.5)
            kickUserBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
            kickUserBtn.Text = "KICK"
            kickUserBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
            kickUserBtn.Font = Enum.Font.GothamBold
            kickUserBtn.TextSize = 16
            kickUserBtn.BorderSizePixel = 0
            kickUserBtn.Parent = userCard
            
            local kickCorner = Instance.new("UICorner")
            kickCorner.CornerRadius = UDim.new(0, 8)
            kickCorner.Parent = kickUserBtn
            
            kickUserBtn.MouseButton1Click:Connect(function()
                kickUserBtn.Text = "‚úÖ KICKED"
                kickUserBtn.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
            end)
        end
        
        scrollFrame.CanvasSize = UDim2.new(0, 0, 0, #sessions * 110)
    end
    
    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 150, 0, 45)
    closeBtn.Position = UDim2.new(0.5, -75, 1, -60)
    closeBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    closeBtn.Text = "CLOSE"
    closeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.TextSize = 18
    closeBtn.BorderSizePixel = 0
    closeBtn.Parent = userListFrame
    
    local closeBtnCorner = Instance.new("UICorner")
    closeBtnCorner.CornerRadius = UDim.new(0, 10)
    closeBtnCorner.Parent = closeBtn
    
    closeBtn.MouseButton1Click:Connect(function()
        userListFrame:Destroy()
    end)
end

if checkWhitelist() or isOwner(LocalPlayer.Name) then
    createMainGUI()
    registerSession()
else
    createAuthGUI()
end
